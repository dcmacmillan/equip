<script>
  window.Squarespace.onInitialize(Y, function(){
    //FAQ toggle
    $('.html-block li').click(function() {
      $(this).toggleClass('open').children('ul').slideToggle(); 
      setTimeout(function(){
        window.dispatchEvent(new Event('resize'));
      }, 300);
    });
  });
</script>
